{
  "name": "ai-knowledge-hub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:worker\"",
    "dev:frontend": "vite",
    "dev:backend": "npm run build:server && node --watch dist/server/server.js",
    "dev:legacy": "node server.cjs",
    "dev:worker": "npm run build:worker && node worker.js",
    "build:worker": "tsc -p tsconfig.worker.json",
    "build:server": "tsc -p tsconfig.server.json",
    "build:vercel": "tsc -p tsconfig.vercel.json",
    "build:api": "tsc -p tsconfig.vercel.json --esModuleInterop",
    "worker": "npm run build:worker && node worker.js",
    "server": "npm run build:server && node dist/server/server.js",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --config=jest.config.js",
    "test:api": "npx jest --config=src/api/tests/jest.config.ts",
    "build": "npm run build:api && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.790.0",
    "@aws-sdk/client-s3": "^3",
    "@aws-sdk/lib-storage": "^3",
    "@aws-sdk/s3-request-presigner": "^3",
    "@distube/ytdl-core": "^4.16.8",
    "@supabase/supabase-js": "^2.39.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/multer": "^1.4.12",
    "@types/uuid": "^10.0.0",
    "@vercel/node": "^3.0.17",
    "axios": "^1.8.4",
    "canvas": "^3.1.0",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "file-type": "^20.4.1",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "formidable": "^3.5.1",
    "lucide-react": "^0.344.0",
    "mammoth": "^1.9.0",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^3.0.3",
    "openai": "^4.93.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf.js-extract": "^0.2.1",
    "pdfjs-dist": "^5.1.91",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.2",
    "tailwind-merge": "^2.1.0",
    "tesseract.js": "^6.0.1",
    "uuid": "^11.1.0",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@jest/globals": "^29.7.0",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^29.5.14",
    "@types/node-cron": "^3.0.11",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "supertest": "^7.1.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
